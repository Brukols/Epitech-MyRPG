#include "my_rpg.h"

game_t *dislay_house_1(game_t *game)
{
    game->scenes->objs->game_object = sort_game_object(game->scenes->objs->game_object);
    display_bg(game);
    display_game_object(game);
    display_inventory(game);
    display_quests(game);
    return (game);
}
